{
  "name": "customer-portal",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "ptit.rentalcar.system",
  "sourceRoot": "apps/customer-portal/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/customer-portal",
        "index": "apps/customer-portal/src/index.html",
        "main": "apps/customer-portal/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/customer-portal/tsconfig.app.json",
        "assets": [
          "apps/customer-portal/src/favicon.ico",
          "apps/customer-portal/src/assets"
        ],
        "styles": [
          "apps/customer-portal/src/styles.css",
          "apps/customer-portal/src/assets/css/owl.carousel.min.css",
          "apps/customer-portal/src/assets/css/animate.css",
          "apps/customer-portal/src/assets/css/owl.theme.default.min.css",
          "apps/customer-portal/src/assets/css/magnific-popup.css",
          "apps/customer-portal/src/assets/css/aos.css",
          "apps/customer-portal/src/assets/css/ionicons.min.css",
          "apps/customer-portal/src/assets/css/bootstrap-datepicker.css",
          "apps/customer-portal/src/assets/css/jquery.timepicker.css",
          "apps/customer-portal/src/assets/css/icomoon.css",
          "apps/customer-portal/src/assets/css/flaticon.css",
          "apps/customer-portal/src/assets/css/style.css",
          "./node_modules/@syncfusion/ej2/material.css"
        ],
        "scripts": [
          "apps/customer-portal/src/assets/js/jquery.min.js",
          "apps/customer-portal/src/assets/js/jquery-migrate-3.0.1.min.js",
          "apps/customer-portal/src/assets/js/popper.min.js",
          "apps/customer-portal/src/assets/js/bootstrap.min.js",
          "apps/customer-portal/src/assets/js/jquery.easing.1.3.js",
          "apps/customer-portal/src/assets/js/jquery.waypoints.min.js",
          "apps/customer-portal/src/assets/js/jquery.stellar.min.js",
          "apps/customer-portal/src/assets/js/owl.carousel.min.js",
          "apps/customer-portal/src/assets/js/jquery.magnific-popup.min.js",
          "apps/customer-portal/src/assets/js/aos.js",
          "apps/customer-portal/src/assets/js/jquery.animateNumber.min.js",
          "apps/customer-portal/src/assets/js/bootstrap-datepicker.js",
          "apps/customer-portal/src/assets/js/jquery.timepicker.min.js",
          "apps/customer-portal/src/assets/js/scrollax.min.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "port": 4300
      },
      "configurations": {
        "production": {
          "browserTarget": "customer-portal:build:production"
        },
        "development": {
          "browserTarget": "customer-portal:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "customer-portal:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/customer-portal/**/*.ts",
          "apps/customer-portal/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/customer-portal/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
